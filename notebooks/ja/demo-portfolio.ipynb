    "        # client.token = \"xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx\"\n",
    "        assert isinstance(self.timeout, int)\n",
    "        client.parameters.timeout = self.timeout\n",
    "        # ソルバーを定義して実行\n",
    "        #         print(\"solving...\")\n",
    "        solver = Solver(client)\n",
    "        result = solver.solve(model)\n",
    "        #         print(\"finish!\")\n",
