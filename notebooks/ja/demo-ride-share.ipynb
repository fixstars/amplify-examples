    "from amplify.constraint import less_equal, one_hot\n",
    "from amplify.client import FixstarsClient\n",
    "\n",
    "from collections import namedtuple\n",
    "from IPython.display import display\n",
    "from ipywidgets import (\n",
    "    Button,\n",
    "    FloatSlider,\n",
