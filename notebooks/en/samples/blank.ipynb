  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "from amplify.client import DWaveClient\n",
    "\n",
    "client = DWaveClient()\n",
    "client.token = \"Please enter your token.\"\n",
